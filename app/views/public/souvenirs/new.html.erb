<h3>投稿ページ</h3>

<%= form_with model: @souvenir do |f| %>

 <h4>商品名</h4>
 <%= f.text_field :item_name %>

 <h4>商品写真</h4>
 <%= f.file_field :image, accept: "image/*" %>

 <h4>商品説明</h4>
 <%= f.text_area :body %>

 <h4>店名または駅名</h4>
 <%= f.text_field :store_name %>

 <h4>県名</h4>
 <%= f.text_field :title %> <br>

 <%= f.submit '投稿'%>
<% end %>



